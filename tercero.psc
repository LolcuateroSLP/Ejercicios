Algoritmo sin_titulo
	Escribir "Ingrese el primer valor";
	leer valor1;
	
	Escribir "Ingrese el segundo valor";
	leer valor2;
	
	Escribir "Ingrese el tercero valor";
	leer valor3;
	
	si(valor1 >= 0) Entonces
		Escribir valor1 * valor2 * valor3
	SiNo
		Escribir valor1 + valor2 + valor3
	FinSi
FinAlgoritmo
